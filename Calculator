# Самописный калькулятор

num1 = int(input())
num2 = int(input())
str1 = input()


ch = "+"
ch1 = "-"
ch2 = "*"
ch3 = "/"

if str1.find(ch) != -1:
    print(num1 + num2)

elif str1.find(ch1) != -1:
    print(num1 - num2)

elif str1.find(ch2) != -1:
    print(num1 * num2)

elif str1.find(ch3) != -1:
    if num2 == 0:
        print("На ноль делить нельзя!")
    else:
        print(num1 / num2)

else:
    print("Неверная операция")
